@model List<PHDS.Entities.Edmx.TimeRecords>
@{ 
    using (var database = new PHDS.Entities.Edmx.EastRiverEntities())
    {
        //from p in database.TimeRecords.AsNoTracking()
        //select
    }
}

@section styles{
    @Styles.Render("~/Content/DataTables/css/datatables")
    <style>
        #details th, #details td {
            white-space: nowrap;
        }

        .dataTables_wrapper {
            font-size: 12px;
        }
    </style>
}

<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title"></h3>
        <div class="box-tools pull-right">
            <!-- Buttons, labels, and many other things can be placed here! -->

            <button class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
        </div><!-- /.box-tools -->
    </div><!-- /.box-header -->
    <div class="box-body">
        <table id="details" class="table table-hover table-bordered table-striped" style="min-width:900px;width:100%">
            @foreach(var item in Model)
            {
                <tr><td>@item.id</td><td>@item.card_id</td><td>@item.sign_time</td></tr>
            }
        </table>
    </div><!-- /.box-body -->
</div><!-- /.box -->

@section scripts{
    @Scripts.Render("~/Scripts/DataTables/datatables")
}
